{
  "version": 2,
  "builds": [
    {
      "src": "apps/frontend/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [

    {
      "src": "^/$",
      "dest": "/apps/frontend/home.html"
    },
    

    {
      "src": "^/([a-zA-Z0-9-_]+)\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot)$",
      "dest": "/apps/frontend/$1.$2"
    },
    {
      "src": "^/public/(.*)$",
      "dest": "/apps/frontend/public/$1"
    },
    

    {
      "src": "^/(servicos|produto|catalogo|sobre|detalhes-produto)$",
      "dest": "/apps/frontend/$1.html"
    },
    

    {
      "src": "^/admin$",
      "dest": "/apps/frontend/admin/login.html"
    },
    {
      "src": "^/admin/([a-zA-Z0-9-_]+)$",
      "dest": "/apps/frontend/admin/$1.html"
    },
    
  
    {
      "src": "^(.*)$",
      "dest": "/apps/frontend/home.html"
    }
  ]
}